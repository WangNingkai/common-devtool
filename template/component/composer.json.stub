{
  "name": "{= pkgName}",
  "type": "library",
  "keywords": [
    "swoole",
    "swoft",
    "swoft-component"
  ],
  "description": "swoft {= name} component",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1"
  },
  "autoload": {
    "psr-4": {
      "{= escapePkgNamespace}\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "{= escapePkgNamespace}Test\\Unit\\": "test/unit/",
      "{= escapePkgNamespace}Test\\Testing\\": "test/testing/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
